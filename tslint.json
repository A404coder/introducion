{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": ["node_modules/**/*.ts"]
    },
    "rules": {
        "ordered-imports": false,
        "member-access": false,
        "no-empty-interface": false,
        "variable-name": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "jsx-no-lambda": false,
        "max-classes-per-file": false,
        "interface-name": false
    }
}
